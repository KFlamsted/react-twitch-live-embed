{
  "name": "react-twitch-live-embed",
  "version": "1.0.0",
  "description": "A lightweight React component for embedding Twitch streams and channels",
  "type": "module",
  "main": "./dist/react-twitch-live-embed.cjs",
  "module": "./dist/react-twitch-live-embed.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/react-twitch-live-embed.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/react-twitch-live-embed.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "react",
    "twitch",
    "embed",
    "stream",
    "player",
    "component",
    "typescript",
    "twitch-embed",
    "twitch-player"
  ],
  "author": "Kristian Flamsted",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "4.1.1",
    "@storybook/addon-a11y": "9.1.10",
    "@storybook/addon-docs": "9.1.10",
    "@storybook/addon-onboarding": "9.1.10",
    "@storybook/addon-vitest": "9.1.10",
    "@storybook/react-vite": "9.1.10",
    "@types/node": "24.6.2",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "@vitejs/plugin-react": "5.0.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "storybook": "9.1.10",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vite-plugin-dts": "4.5.4",
    "vitest": "3.2.4",
